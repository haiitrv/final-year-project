<%- include('../layouts/header') %> 

<h1>Category</h1>

<table class="table">
    <thead>
        <th>ID</th>
        <th>Category</th>
        <th>Description</th>
        <th>Created Date</th>
        <th>Created By</th>
    </thead>
    <tbody>
        <% categories.forEach(category=> { %>
            <tr>
                <td>
                    <%= category.id %>
                </td>
                <td>
                    <a href="/aad/category/<%= category.id %>">
                    <%= category.name %>
                    </a>
                </td>
                <td>
                    <%= category.description %>
                </td>
                <td>
                    <%= category.createdAt %> 
                </td>
                <td>
                    <%= category.user %> 
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>


<%- include('../layouts/footer') %> 