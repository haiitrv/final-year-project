<%- include('../layouts/header') %> 

<h1>Course</h1>

<table class="table">
    <thead>
        <th>ID</th>
        <th>Course</th>
        <th>Description</th>
        <th>Created Date</th>
        <th>Category</th>
    </thead>
    <tbody>
        <% courses.forEach(course => { %>
            <tr>
                <td>
                    <%= course.id %>
                </td>
                <td>
                    <%= course.name %>
                    </a>
                </td>
                <td>
                    <%= course.description %>
                </td>
                <td>
                    <%= course.createdAt %> 
                </td>
                <td>
                    <%= course.category %> 
                </td>
                
            </tr>
            <% }) %>
    </tbody>
</table>


<%- include('../layouts/footer') %> 